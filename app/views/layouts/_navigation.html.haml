#header-landing{"data-magellan-destination" => "home"}
  .fixed.sticky
    %nav#nav-menu.top-bar{"data-magellan-expedition" => "fixed", "data-topbar" => "", :role => "navigation"}
      %ul.title-area
        %li.name
          %h1
            %a{:href => "#{root_path}"}
              %i.fa.fa-bullseye
              Reed Nelson-Saunders
        %li.toggle-topbar.menu-icon
          %a{:href => "#"}
            %span Menu
      %section#top-main-nav.top-bar-section.menus
        %ul.right.scroll-nav
          %li.active{"data-magellan-arrival" => "home"}
            = link_to 'Blog', articles_path
          %li{"data-magellan-arrival" => "services"}
            = link_to 'Projects', projects_path
          %li{"data-magellan-arrival" => "portfolio"}
            = link_to 'Log In', login_path
          %li{"data-magellan-arrival" => "team"}
            = link_to 'Sign up', signup_path
          - if user_signed_in?
            %li{"data-magellan-arrival" => "team"}
            = link_to 'Log out', logout_path
